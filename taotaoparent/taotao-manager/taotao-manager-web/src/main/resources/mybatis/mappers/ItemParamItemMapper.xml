<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taotao.manager.mapper.ItemParamItemMapper">

	<update id="updateItemParemItemByItemId" parameterType="com.taotao.manager.pojo.ItemParamItem">
		UPDATE tb_item_param_item
		SET param_data = #{paramData},
			updated = #{updated}
		WHERE item_id = #{itemId}
	</update>
	

	
</mapper>
